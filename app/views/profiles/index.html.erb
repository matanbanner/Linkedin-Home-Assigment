<p id="notice"><%= notice %></p>

<h1>Profiles</h1>

<table>
  <thead>
    <tr>
      <th>Uid</th>
      <th>Name</th>
      <th>Title</th>
      <th>Current position</th>
      <!-- <th>Summary</th> -->
      <!-- <th>Experience</th> -->
      <!-- <th>Education</th> -->
      <th>Score</th>
      <th>Url</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @profiles.each do |profile| %>
      <tr>
        <td><%= profile.uid %></td>
        <td><%= profile.name %></td>
        <td><%= profile.title %></td>
        <td><%= profile.current_position %></td>
        <!-- <td><%= profile.summary %></td> -->
        <!-- <td><%= profile.experience %></td> -->
        <!-- <td><%= profile.education %></td> -->
        <td><%= profile.score %></td>
        <td><%= profile.url %></td>
        <td><%= link_to 'Show', profile %></td>
        <td><%= link_to 'Edit', edit_profile_path(profile) %></td>
        <td><%= link_to 'Destroy', profile, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Profile', new_profile_path %>
